
  {{#each articles}}
  <div data-_id={{this._id}} class="card">
    <div class="card-header">
      <h3>
        <a class="article-link" target="_blank" rel="noopener noreferrer" href={{this.link}}>{{this.title}}</a>
      </h3>
    </div>
    <div class="card-body">{{this.excerpt}}</div>
  </div>
  {{/each}}